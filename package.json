{
  "name": "domless",
  "version": "0.0.1",
  "description": "A way to build web interfaces without relying on the DOM. Leverages HTML Canvas and Phaser.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-demo": "webpack --config ./demo/webpack-demo.config.js",
    "test": "nyc --reporter=lcov --reporter=text mocha-webpack --webpack-config ./demo/test/webpack-demo-test.config.js --recursive demo/test",
    "serve-demo": "webpack-dev-server --config ./demo/webpack-demo.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/galarant/domless.git"
  },
  "keywords": [
    "canvas"
  ],
  "author": "Jeff Revesz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/galarant/domless/issues"
  },
  "homepage": "https://github.com/galarant/domless",
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-env": "1.6.0",
    "canvas-prebuilt": "1.4.0",
    "chai": "3.5.0",
    "esdoc": "0.5.2",
    "file-loader": "0.11.1",
    "image-webpack-loader": "3.3.1",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jsdom": "9.12.0",
    "mocha": "3.3.0",
    "mocha-webpack": "0.7.0",
    "nyc": "11.0.3",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-chain": "3.3.0",
    "webpack-dev-server": "2.4.2",
    "webpack-node-externals": "1.5.4",
    "xml-loader": "1.2.1"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "mocha-webpack": "0.7.0",
    "phaser-ce": "2.8.2"
  },
  "nyc": {
    "include": [
      "demo/**/*.js",
      "src/**/*.js"
    ],
    "instrument": false,
    "report-dir": "./coverage",
    "sourceMap": false
  }
}
